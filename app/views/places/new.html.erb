<main class="" id="main-collapse">
 
 

<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<div class="panel panel-default">
				<div class="panel-heading">
					<div class="text-center">
						<span style="font-size:24px;">Place</span>
					</div>
				</div>
			
				<div class="panel-body">			
					<!-- ここからフォーム -->
					<%= form_for @place do |f| %>
						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>title</label>
                                <%= f.text_field :title, placeholder: "タイトルを記入してください", class: "form-control", required: "true" %>
                            	</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>region</label>
                                <%= f.text_field :author, placeholder: "地域を記入してください", class: "form-control", required: "true" %>
                            	</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>reason</label>
                                <%= f.text_area :description, rows: 10, placeholder: "1番の理由を記入してください", class: "form-control", required: "true" %>
                            </div> 
							</div>
						</div>
						<br>

						<div class="col-md-12 select">
              <div class="form-group">
                <label>photo</label>
            <div class="field">
              <%= f.file_field :photo %>
            </div>
            </div>
            </div>
            <br><br>

						<div class="actions">
							<%= f.submit "Save", class: "btn btn-danger" %>
						</div>
					<% end %>
				</div>
			</div>
		</div>
	</div>

</div>



</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

