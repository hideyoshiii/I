<header class="">
  <div class="navbar navbar-default visible-xs">
    <button type="button" class="navbar-toggle collapsed">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a href="/" class="navbar-brand">my&nbsp;I&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;あなたのNo.1</a>
  </div>

  <nav class="sidebar">
    <div class="navbar-collapse" id="navbar-collapse">
      <div class="site-header hidden-xs">
          <a class="site-brand" href="/" title="">
            <h2>
              <%= image_tag 'mashup-logo.svg', :class =>'site-logo' %>
              my&nbsp;I
            </h2>
          </a>
        <p>あなたのNo.1</p>
      </div>
      <ul class="nav">
        <li><%= link_to "Book", books_path %></li>
        <li><%= link_to "Movie", movies_path %></li>
        <li><%= link_to "Song", songs_path %></li>
        <li><%= link_to "Dish", dishes_path %></li>
        <li><%= link_to "Place", places_path %></li>
        <li><%= link_to "my I", goods_path %></li>


      </ul>

      <nav class="nav-footer">
        <% if (user_signed_in?) %>
        <%= link_to user_path(current_user) do %>
        <%= image_tag current_user.image, :class => "img-circle profile-0" %>
        &nbsp;&nbsp;<%= current_user.name %>
        <% end %>
        <br>
        <%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
        <% else %>
        <%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>
        <% end %>
        <p class="nav-footer-social-buttons">
          <a class="fa-icon" href="https://www.instagram.com/I_myonlyNo.1/" title="">
            <i class="fa fa-instagram"></i>
          </a>
          <a class="fa-icon" href="https://www.facebook.com/myi.no1/" title="">
            <i class="fa fa-facebook" aria-hidden="true"></i>
          </a>
          <a class="fa-icon" href="https://twitter.com/I_myonly_No1 title=">
            <i class="fa fa-twitter"></i>
          </a>
        </p>
        <p>© You | Website created with <a href="http://www.mashup-template.com/" title="Create website with free html template">Mashup Template</a>/<a href="https://www.unsplash.com/" title="Beautiful Free Images">Unsplash</a></p>
      </nav>  
    </div> 
  </nav>
</header>
<main class="" id="main-collapse">

<h3>my&nbsp;I</h3>
<br>
<div class="hero-full-wrapper">
  <div class="grid">
  <div class="gutter-sizer"></div>
    <div class="grid-sizer"></div>
    <% @goods.each do |good| %>
    <div class="grid-item">
      <%= image_tag good.photo.url, :class =>'img-responsive' %>
      <%= link_to good_path(good) do %>
      <div class="project-description">
        <div class="project-text-holder">
          <div class="project-text-inner">
            <h4><%= good.title %></h4>
            <p>Discover more</p>
          </div>
        </div>
      </div>
      <% end %>
    </div>
   <% end %>  
  </div>
</div>


<script>
  document.addEventListener("DOMContentLoaded", function (event) {
     masonryBuild();
  });
</script>

 

</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
