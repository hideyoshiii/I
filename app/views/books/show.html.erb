<header class="">
  <div class="navbar navbar-default visible-xs">
    <button type="button" class="navbar-toggle collapsed">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a href="/" class="navbar-brand">I&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;あなたのNo.1</a>
  </div>

  <nav class="sidebar">
    <div class="navbar-collapse" id="navbar-collapse">
      <div class="site-header hidden-xs">
          <a class="site-brand" href="/" title="">
            <h2>
              <%= image_tag 'mashup-logo.svg', :class =>'site-logo' %>
              &nbsp;I
            </h2>
          </a>
        <p>あなたのNo.1</p>
      </div>
      <ul class="nav">
        <li><%= link_to "Book", books_path %></li>
        <li><%= link_to "Movie", movies_path %></li>
        <li><%= link_to "Song", songs_path %></li>
        <li><%= link_to "Dish", dishes_path %></li>
        <li><%= link_to "Place", places_path %></li>
        <li><%= link_to "Your I", goods_path %></li>


      </ul>

      <nav class="nav-footer">
        <% if (user_signed_in?) %>
        <%= link_to user_path(current_user) do %>
        <%= image_tag current_user.image, :class => "img-circle profile-0" %>
        &nbsp;&nbsp;<%= current_user.name %>
        <% end %>
        <br>
        <%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
        <% else %>
        <%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>
        <% end %>
        <p class="nav-footer-social-buttons">
          <a class="fa-icon" href="https://www.instagram.com/I_myonlyNo.1/" title="">
            <i class="fa fa-instagram"></i>
          </a>
          <a class="fa-icon" href="/" title="">
            <i class="fa fa-facebook" aria-hidden="true"></i>
          </a>
          <a class="fa-icon" href="https://twitter.com/I_myonly_No1 title=">
            <i class="fa fa-twitter"></i>
          </a>
        </p>
        <p>© You | Website created with <a href="http://www.mashup-template.com/" title="Create website with free html template">Mashup Template</a>/<a href="https://www.unsplash.com/" title="Beautiful Free Images">Unsplash</a></p>
      </nav>  
    </div> 
  </nav>
</header>
<main class="" id="main-collapse">

<div class="img-show">
<%= image_tag @book.photo.url, :class =>'img-responsive' %>
<br><br>
<h3><%= @book.title %></h3>
<P><%= @book.author %></p>
<br><br>
<p style="margin-left:50px;margin-right:50px"><%= @book.description %></p>
<br><br>
<%= link_to user_path(@book.user) do %>
<%= image_tag @book.user.image, :class => "img-circle profile-1" %>
<% end %>
&nbsp;&nbsp;<%= @book.user.name %>
<% if @book.user == current_user %>
<br><br>
<%= link_to "編集する", edit_book_path(@book), :class => "btn btn-default" %>
<% end %>

</div>

</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

